// main gallery start

section.section.main-gallery.no-padding-top.no-padding-bottom
	.container
		.row.align-items-end.margin-bottom
			.col-lg-9
				.heading.primary-heading
					+primary-heading('We Created', 'Project', 'Cases')
					p Streamer fish California halibut Pacific saury. Slickhead grunion lake trout. Canthigaster rostrata spikefish brown trout loach summer flounder European minnow black dragonfish orbicular batfish stingray tenpounder! Flying
			.col-lg-3
				.button-block.title-button
					+btn-transp('Discover')

	// gallery start
	.gallery.row.no-gutters

		// item start
		.col-xl-3.gallery-item.col-sm-6
			a.gallery-link(href="img/gallery-1.jpg" data-fancybox="main-gallery")
				.overlay
				picture
					source(srcset='img/gallery-1.jpg' media='(min-width: 992px)')
					img.img-bg(src='img/gallery-1.jpg' alt='img')
			.description
				span Solar Factory in NY
