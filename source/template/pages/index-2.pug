extends ../layouts/primary-map.pug

block vars

	-var page = 'index-2'

	-var type = 'front'

block header

	include ../blocks/_mobile-menu/_mobile-menu

	// header start

	header#header.header.header--style-2
		.container-fluid
			.row
				.col-12.column

					// logo block start
					.logo-block
						a.logo(href="index.html")
							+svg('logo')
							span R - energy
					// logo block end

					// menu block start
					.menu-block
						include ../blocks/_main-menu/_main-menu
					// menu block end

					// lang block start
					.lang-block
						ul.lang-select
							li.lang-item.active #[span EN]
								ul.sub-list
									li.lang-item
										a(href="#") French
									li.lang-item
										a(href="#") Spanish
									li.lang-item
										a(href="#") Deutsch
									li.lang-item
										a(href="#") Russian
						+btn-transp('Get In Touch')

						include ../blocks/_menu-trigger/_menu-trigger

					// lang block end

	// header end

block main

	include ../blocks/_promo--style-2/_promo--style-2

	// about-welcome-style-2 start

	section.section.about-welcome.about-welcome--style-2.no-padding-bottom
		.container
			.row.align-items-center
				.col-xl-4.col-lg-5
					.title-block
						span About
						h2.title Welcome to Our R-Energy Company
						.icon-holder
							+svg('logo')
				.col-xl-7.offset-xl-1.col-lg-6.offset-lg-1
					.description
						h3.title We provide future of energy
						p Streamer fish California halibut Pacific saury. Slickhead grunion lake trout. Canthigaster rostrata spikefish brown trout loach summer flounder
						p European minnow black dragonfish orbicular batfish stingray tenpounder! Flying characin: herring. Moses sole sea snail grouper discus. European eel slender snipe eel. Gulper eel dealfish ocean sunfish rohu yellow-and-black triplefin Atlantic saury swordfish southern
						+btn-transp('More about')

	// about-welcome-style-2 end

	// brands start

	section.section.brands.no-padding-bottom
		.container
			.row
				.col-12
					include ../blocks/_brands-slider/_brands-slider

	// brands end

	include ../blocks/_services--style-2/_services--style-2

	include ../blocks/_cases--style-2/_cases--style-2

	include ../blocks/_benefits/_benefits

	include ../blocks/_platform/_platform

	// pricing start

	section.section.pricing.no-padding-bottom
		.container
			.row
				.col-12
					.heading.primary-heading
						+primary-heading('Pricing', 'Pricing Plan', 'for you')
						p Members are proactive in both undertaking and applying animal welfare scientific research, contributing being a recognised organisation in animal welfare best-practice.

